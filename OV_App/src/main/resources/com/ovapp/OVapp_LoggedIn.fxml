            <Tooltip fx:id="deLanguageButtonToolTip" style="-fx-text-fill: #FFFFFF;" text="auf Deutsch umstellen" />
         </tooltip></Button>
         <Button fx:id="logOutButton" layoutX="801.0" layoutY="13.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" prefHeight="52.0" prefWidth="202.0" style="-fx-border-color: #ff6600;" text="Uitloggen">
