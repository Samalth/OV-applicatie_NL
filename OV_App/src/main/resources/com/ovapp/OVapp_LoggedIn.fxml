   </padding>
   <Pane prefHeight="623.0" prefWidth="1238.0">
      <children>
         <Button fx:id="enLanguageButton" layoutX="630.0" layoutY="533.0" mnemonicParsing="false" onAction="#onEnLanguageButtonClick" prefHeight="41.0" prefWidth="76.0" style="-fx-background-image: url('uk_flag.png'); -fx-background-size: cover;">
         <tooltip>
            <Tooltip fx:id="enLanguageButtonToolTip" style="-fx-text-fill: #FFFFFF;" text="Switch to English" />
         </tooltip></Button>
         <Button fx:id="nlLanguageButton" layoutX="552.0" layoutY="533.0" mnemonicParsing="false" onAction="#onNlLanguageButtonClick" prefHeight="41.0" prefWidth="76.0" style="-fx-background-image: url('nl_flag.png');-fx-background-size: cover;">
          <tooltip>
            <Tooltip fx:id="nlLanguageButtonToolTip" style="-fx-text-fill: #FFFFFF;" text="wissel naar Nederlands" />
          </tooltip></Button>
         <Button fx:id="deLanguageButton" layoutX="708.0" layoutY="533.0" mnemonicParsing="false" onAction="#onDuLanguageButtonClick" prefHeight="41.0" prefWidth="76.0" style="-fx-background-image: url('de_flag.png'); -fx-background-size: cover;">
