               <Font name="Calibri" size="22.0" />
            </font>
         </Text>
         <Text fx:id="MeansOfTransportText" layoutX="19.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Vervoermiddel">
