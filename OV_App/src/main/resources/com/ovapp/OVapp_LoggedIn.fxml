               <Font name="Calibri" size="22.0" />
            </font>
         </Text>
         <Text fx:id="DepartureTimeText" layoutX="19.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Vertrektijd">
