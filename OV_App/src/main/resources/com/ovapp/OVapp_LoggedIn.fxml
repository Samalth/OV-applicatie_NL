            <tooltip>
               <Tooltip fx:id="dateLabelToolTip" style="-fx-text-fill: #FFFFFF;" text="de huidige datum" />
            </tooltip></Label>
         <Label fx:id="clockLabel" layoutX="493.0" layoutY="12.0" prefHeight="41.0" prefWidth="82.0" style="-fx-border-color: #ff6600;" text="klokLabel">
