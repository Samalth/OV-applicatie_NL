            <tooltip>
               <Tooltip fx:id="modeToolTip" style="-fx-text-fill: #FFFFFF;" text="Klik hier om te wisselen naar donkere/lichte modus" />
            </tooltip></Button>
         <ImageView fx:id="imgMode" cache="true" fitHeight="52.0" fitWidth="27.0" layoutX="752.0" layoutY="27.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
