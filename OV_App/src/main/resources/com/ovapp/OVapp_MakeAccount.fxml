<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="parent" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ovapp.MakeAccountController">
    <children>
        <AnchorPane minHeight="76.0" prefHeight="76.0" prefWidth="1238.0" style="-fx-background-color: #ff6600;">
            <children>
                <Label fx:id="dateLabel" alignment="CENTER" layoutX="32.0" layoutY="19.0" prefHeight="38.0" prefWidth="112.0" style="-fx-border-color: #FF6600; -fx-background-color: #DEDEDE;">
                    <font>
                        <Font name="Calibri" size="15.0" />
                    </font>
                    <Tooltip fx:id="dateLabelToolTip" styleClass="tooltip" text="de huidige datum" />
                </Label>
                <Label fx:id="clockLabel" layoutX="153.0" layoutY="19.0" prefHeight="38.0" prefWidth="112.0" style="-fx-border-color: #ff6600; -fx-background-color: #DEDEDE;">
                    <font>
                        <Font name="Calibri" size="15.0" />
                    </font>
                    <tooltip>
                        <Tooltip fx:id="clockLabelToolTip" style="-fx-text-fill: #FFFFFF;" text="de huidige tijd" />
                    </tooltip>
                </Label>
                <Button alignment="CENTER" layoutX="296.0" layoutY="13.0" mnemonicParsing="false" onAction="#onChangeModeClick" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: #444444;" styleClass="dark-mode" stylesheets="@../../lightmode.css">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <tooltip>
                        <Tooltip fx:id="modeToolTip" style="-fx-text-fill: #FFFFFF;" text="Klik hier om te wisselen naar donkere/lichte modus" />
