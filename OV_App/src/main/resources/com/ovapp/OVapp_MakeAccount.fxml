                        <Tooltip fx:id="enLanguageButtonToolTip" styleClass="tooltip" text="Switch to English" />
                    </tooltip>
                </Button>
                <Button fx:id="nlLanguageButton" layoutX="497.0" layoutY="16.0" mnemonicParsing="false" onAction="#onNlLanguageButtonClick" prefHeight="41.0" prefWidth="76.0" style="-fx-background-image: url('nl_flag.png');-fx-background-size: cover;">
                    <tooltip>
                        <Tooltip fx:id="nlLanguageButtonToolTip" styleClass="tooltip" text="wissel naar Nederlands" />
                    </tooltip>
                </Button>
            <Button fx:id="previousButton" layoutX="1086.0" layoutY="8.0" mnemonicParsing="false" onAction="#previousButtonOnAction" prefHeight="57.0" prefWidth="180.0" text="&lt;-Vorige pagina " />
            </children>
        </AnchorPane>

        <Pane prefHeight="720.0" prefWidth="1280.0">
            <children>
                <Label fx:id="loginMessageLabel" layoutX="14.0" layoutY="196.0" prefHeight="277.0" prefWidth="425.0" textFill="RED">
                </Label>
                <TextField fx:id="usernameTextField" layoutX="464.0" layoutY="144.0" prefHeight="94.0" prefWidth="379.0" promptText="Gebruikersnaam">
                </TextField>
                <PasswordField fx:id="passwordPasswordField" layoutX="464.0" layoutY="261.0" prefHeight="94.0" prefWidth="379.0" promptText="Wachtwoord">
                </PasswordField>
                <ImageView fx:id="travelYRouteImg" cache="true" fitHeight="169.0" fitWidth="203.0" layoutX="1063.0" layoutY="21.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../TravelYRouteBlanc.png" />
                    </image>
                </ImageView>
                <Button fx:id="logInButton" layoutX="883.0" layoutY="204.0" mnemonicParsing="false" onAction="#logInButtonOnAction" prefHeight="63.0" prefWidth="231.0" style="-fx-border-color: #0863b5;" text="Inloggen">
                    <font>
                        <Font name="Calibri" size="22.0" />
                    </font>
                    <tooltip>
                        <Tooltip fx:id="logInButtonToolTip" styleClass="tooltip" text="klik hier om in te loggen" />
                    </tooltip>
                </Button>
                <Label layoutX="416.0" layoutY="65.0" prefHeight="41.0" prefWidth="471.0" style="-fx-background-color: #FFFFCC; -fx-font-weight: bold;" text="Voer uw Nieuwe inloggegevens in. heeft u al een account? klik op &quot;vorige pagina&quot; " />
